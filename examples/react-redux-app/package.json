{
  "name": "react-bare-minimum",
  "version": "1.0.1",
  "description": "A bare minimum setup to work with react. Compiling and bundling, nothing else.",
  "repository": {
    "url": "git@github.com:krasimir/react-bare-minimum.git",
    "type": "git"
  },
  "scripts": {
    "start": "concurrently \"watchify ./src/client/App.jsx -o ./build/client/app.js -t babelify -v\" \"babel ./src/server --out-dir ./build/server --watch\" \"nodemon ./build/server/server.js\""
  },
  "author": "Krasimir Tsonev <info@krasimirtsonev.com>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babelify": "8.0.0",
    "browserify": "14.5.0",
    "concurrently": "3.6.0",
    "dactory": "0.6.4",
    "express": "4.16.3",
    "fsevents": "1.2.4",
    "nan": "2.11.0",
    "nodemon": "1.17.5",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "watchify": "3.9.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
