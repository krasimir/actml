{
  "name": "dialectica",
  "version": "0.1.2",
  "description": "Like jsx but for your domain logic",
  "main": "lib/dialectica.js",
  "scripts": {
    "test": "BABEL_ENV=development ./node_modules/.bin/jest \"(.*)\\.spec\\.js\"",
    "minify": "uglifyjs ./lib/dialectica.js -o ./lib/dialectica.min.js",
    "build": "PROCESS=build ./node_modules/.bin/rollup -c ./rollup.config.js && npm run minify",
    "release": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krasimir/dialectica.git"
  },
  "keywords": [
    "dialect",
    "jsx"
  ],
  "author": "Krasimir Tsonev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krasimir/dialectica/issues"
  },
  "homepage": "https://github.com/krasimir/dialectica#readme",
  "dependencies": {
    "@babel/core": "7.0.0-rc.3",
    "@babel/preset-env": "7.0.0-rc.3",
    "@babel/preset-react": "7.0.0-rc.3",
    "rollup": "0.65.0",
    "rollup-plugin-babel": "4.0.0-beta.8"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-0",
    "babel-jest": "23.4.2",
    "jest": "23.5.0",
    "regenerator-runtime": "0.12.1"
  }
}
